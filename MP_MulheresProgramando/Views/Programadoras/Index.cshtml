@model IEnumerable<MP_MulheresProgramando.Models.Programadora>


<h3>Programadoras</h3>
<hr />

<p>
    <a asp-action="Create">Incluir nova Programadora</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NomeDev)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Linguagens)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NomeDev)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Linguagens.Nome)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Editar</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Detalhes</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Excluir</a>
            </td>
        </tr>
}
    </tbody>
</table>
